set( PACKAGE_VERSION "@MUJINVISION_VERSION@" )
if( "${PACKAGE_FIND_VERSION}" VERSION_EQUAL "@MUJINVISION_VERSION@")
  set(PACKAGE_VERSION_EXACT 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}.${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@MUJINVISION_SOVERSION@" )
  set(PACKAGE_VERSION_COMPATIBLE 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@MUJINVISION_VERSION_MAJOR@" )
  if( "${PACKAGE_FIND_VERSION_MINOR}" EQUAL 0 )
    # user has not specified a minor version, so return compatible
    set(PACKAGE_VERSION_COMPATIBLE 1)
  endif()
endif()
